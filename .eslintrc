{
  "env": {
    "browser": true,
    "es6": true
  },

  "parser": "babel-eslint",

  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },

  "rules": {
    "semi": ["error", "never"],
    "indent": ["error", 2],
    "comma-dangle": 0,
    "no-var": "error",
    "no-class-assign": 0
  }
}
